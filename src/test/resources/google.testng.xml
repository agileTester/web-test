<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Google Test Suit">

	<test name="Google Search Selenium Test">
		<classes>
			<class name="pl.webtest.test.google.GoogleSearchSeleniumTest" />
		</classes>
	</test>

	<test name="Google Search Page Object Design Pattern Search">
		<parameter name="searchString" value="selenium" />
		<parameter name="expectedResult" value="Selenium" />
		<classes>
			<class name="pl.webtest.test.google.GoogleSearchPageTest" />
		</classes>
	</test>
	
	<test name="Google Translate Selenium Test">
		<parameter name="fromLang" value="en" />
		<parameter name="toLang" value="pl" />
		<parameter name="translateString" value="selenium" />
		<parameter name="expectedResult" value="selen" />
		<classes>
			<class name="pl.webtest.test.google.GoogleTranslateSeleniumTest" />
		</classes>
	</test>	
		
</suite>